  "tokenColors": [
    {
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "invalid.deprecated",
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#D2FFF8"
      }
    },
    {
      "scope": [
        "variable",
        "string constant.other.placeholder",
        "entity.name.function",
        "punctuation.definition.interpolation.end.bracket.curly.scss",
